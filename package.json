{
  "name": "pos-api",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/dev",
    "dev": "DEBUG=*,-prisma:*,-express:router* nodemon ./bin/dev --ignore ./api/v1/prisma/db --ignore ./uploads --inspect",
    "migrate": "npx prisma migrate dev",
    "build": "npx prisma migrate deploy && npx prisma generate && npx prisma db seed",
    "studio": "npx prisma studio"
  },
  "dependencies": {
    "@prisma/client": "^3.8.1",
    "@squoosh/lib": "^0.4.0",
    "axios": "^0.24.0",
    "bcrypt": "^5.0.1",
    "cors": "^2.8.5",
    "debug": "~4.3.3",
    "dinero.js": "^1.9.1",
    "express": "~4.16.1",
    "express-openapi": "^10.0.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "~1.9.1",
    "multer": "^1.4.4",
    "sqlite3": "^5.0.2"
  },
  "devDependencies": {
    "nodemon": "^2.0.15",
    "prisma": "^3.8.1"
  },
  "prisma": {
    "schema": "api/v1/prisma/schema.prisma",
    "seed": "node api/v1/prisma/seed.js"
  }
}
